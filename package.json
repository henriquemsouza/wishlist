{
    "name": "wishlist",
    "version": "1.0.0",
    "license": "MIT",
    "main": "src/index.ts",
    "author": "Henrique martins de souza",
    "description": "A basic Wishlist Serverless Lambda",
    "scripts": {
        "lint": "eslint src/ --ext .ts",
        "clean": "rm -rf dist/ && rm -rf .build/",
        "routes": "node -r ts-node/register ./helpers/ShowServerRoutes.ts",
        "start:dev": "SLS_DEBUG=* && node --inspect ./node_modules/.bin/sls offline --watch --config serverless-dev.yml -s dev --noPrependStageInUrl",
        "build": "tsc",
        "build:webpack": "webpack --config webpack.config.js",
        "migration:up": "yarn typeorm migration:run -c $NODE_ENV",
        "migration:down": "yarn typeorm migration:revert -c  $NODE_ENV",
        "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config -c $NODE_ENV",
        "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -c $NODE_ENV"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.84",
        "@types/aws-serverless-express": "^3.3.5",
        "@types/chai": "^4.2.22",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/faker": "^5.5.9",
        "@types/mocha": "^9.0.0",
        "@types/pg": "^8.6.1",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "chai": "^4.3.4",
        "eslint": "^8.0.1",
        "express-routes-catalogue": "^1.1.0",
        "express-winston": "^4.2.0",
        "mocha": "^9.1.3",
        "nodemon": "^2.0.14",
        "serverless": "^2.64.1",
        "serverless-dotenv-plugin": "^3.10.0",
        "serverless-offline": "^8.2.0",
        "serverless-plugin-typescript": "^2.1.0",
        "ts-node": "^10.3.0",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.12.0",
        "tslint": "^6.1.3",
        "typescript": "^4.4.4",
        "webpack": "^5.10.0",
        "webpack-cli": "^4.2.0",
        "winston": "^3.3.3"
    },
    "dependencies": {
        "@types/swagger-ui-express": "^4.1.3",
        "aws-serverless-express": "^3.4.0",
        "axios": "^0.24.0",
        "class-validator": "^0.13.2",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "http-status": "^1.5.0",
        "inversify": "^6.0.1",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.3.0",
        "typeorm": "^0.2.41",
        "typeorm-naming-strategies": "^2.0.0",
        "typeorm-seeding": "^1.6.1"
    }
}
