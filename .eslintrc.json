{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "eol-last": ["error", "always"],
        "camelcase": "off",
        "dot-notation": "off",
        "no-useless-constructor": "off",
        "no-unused-expressions": "off",
        "@typescript-eslint/no-explicit-any": "off"
    }
}
